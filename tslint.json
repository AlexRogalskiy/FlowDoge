{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "object-literal-sort-keys": false,
        "ordered-imports": [
            false
        ],
        "semicolon": [
            true,
            "never"
        ],
        "variable-name": [
            false
        ],
        "interface-name": [
            false
        ],
        "object-literal-key-quotes": [
            false
        ],
        "member-access": false,
        "trailing-comma": [
            false
        ],
        "arrow-parens": false
    },
    "rulesDirectory": []
}
